<!-- Control launch file -->
<launch>

  <node name="puzzlebot_controller" pkg="ros_stack_sim_jackal" type="ros_stack_sim_jackal_node"/>

  <node name="odom" pkg="ros_stack_sim_jackal" type="localization.py"/>

  <include file="$(find ros_stack_sim_jackal)/launch/include/robot/puzzlebot_control.launch"/>"

  <!--Teleop Keyboard-->
  <node name="teleop" pkg="teleop_twist_keyboard" type="teleop_twist_keyboard.py"/>

  <!-- To be able to teleop my robot through RVIZ using interactive markers -->
  <node pkg="interactive_marker_twist_server" type="marker_server"
        name="twist_marker_server">
    <remap from="twist_marker_server/cmd_vel" to="/cmd_vel"/>
  </node>
</launch>